table.customMod { background-color: #ffffcc; border:outset 1px black; }
table.customMod td { padding:5px; }
table.customMod td table td { padding:2px; }
table.customMod td.header { background-color: #ffd700; padding: 2px; }
table.customMod td.header a { color:Black; font-weight:bold; }
.toolbar { background-color:white; color:black; background-color:#7891AC; display:inline; background-image:url(../../img/bg.gif);}
table.datos 
table.datos tr.head th, table.datos th {background-color:#7891AC; }
table.datos tr td { background-color:#a3bbd2; padding:2px 5px 2px 5px; }
table.datos tr.alt td { background-color: #bacde2; }