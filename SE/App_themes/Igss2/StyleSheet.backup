table, table td { border-collapse:collapse; }
table td { }
table.std .nopad { padding:0px; margin:0px; }
table.std td { border-collapse:collapse; border:solid 1px #efefef; }
table.std th { background-color:#2f745E; color:#ffffff; padding:0px; }
td.nim { background-color:#efefef; }
